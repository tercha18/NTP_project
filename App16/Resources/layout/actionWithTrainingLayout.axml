			android:id="@+id/editTraining" />
		<Button
			android:text="Trenuj"
			android:layout_width="350px"
			android:layout_height="wrap_content"
			android:layout_gravity="center"
			android:background="@drawable/buttondesign"
			android:id="@+id/doTraining" />
</LinearLayout>
